{
  "name": "aideyekit",
  "license": "GPL-3.0-only",
  "private": false,
  "version": "0.0.2",
  "main": "./dist/aideyekit.cjs",
  "module": "./dist/aideyekit.mjs",
  "types": "./dist/aideyekit.d.ts",
  "homepage": "https://aideyejs.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DoughnutsNCookies/AidEyeKit-SDK.git"
  },
  "author": "Sean Chuah <chuahtseyung2002@gmail.com>",
  "bugs": {
    "url": "https://github.com/DoughnutsNCookies/AidEyeKit-SDK/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/aideyekit.d.ts",
      "require": "./dist/aideyekit.cjs",
      "import": "./dist/aideyekit.mjs"
    },
    "./dist/aideye.css": {
      "import": "./dist/aideye.css",
      "require": "./dist/aideye.css"
    }
  },
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "test": "vitest",
    "format": "prettier . --write"
  },
  "files": [
    "!tests/**/*",
    "!docs/**/*",
    "dist/**/*",
    "!dist/**/*.js.map"
  ],
  "devDependencies": {
    "@types/jsdom": "^21.1.2",
    "@vitest/coverage-c8": "^0.32.0",
    "dts-bundle-generator": "^8.0.1",
    "postcss": "^8.4.29",
    "postcss-scss": "^4.0.7",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.3"
  },
  "keywords": [
    "aideyekit",
    "aideye",
    "onboarding",
    "guide",
    "visual guide",
    "overlay",
    "tooltip",
    "walkthrough",
    "product tour",
    "product walkthrough",
    "product guide",
    "product tutorial",
    "product demo",
    "user onboarding"
  ]
}
